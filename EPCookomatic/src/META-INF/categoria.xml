<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence/orm
          http://xmlns.jcp.org/xml/ns/persistence/orm_2_2.xsd"
                 version="2.2">
    <entity class="org.milaifontanals.cookomatic.model.cuina.Categoria" access="FIELD" metadata-complete="true">
        <table name="CATEGORIA"/>
        
        <named-query name="Categories">
            <query>select c from Categoria c</query>
        </named-query>

        <attributes>
            <id name="codi">
                <column name="CODI"/>
            </id>
            <basic name="nom" optional="false">
                <column name="NOM"/>
            </basic>
            <basic name="colorInt">
                <column name="COLOR"/>
            </basic>
            
            <transient name="color"></transient>
        </attributes>
    </entity>

</entity-mappings>
